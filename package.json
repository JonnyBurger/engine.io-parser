{
  "name": "engine.io-parser",
  "description": "Parser for the client for the realtime Engine",
  "license": "MIT",
  "version": "3.0.0-alpha.1",
  "main": "dist/index.js",
  "homepage": "https://github.com/socketio/engine.io-parser",
  "files": [
    "dist/"
  ],
  "engines": {
    "node" : ">=8.0.0"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --parser typescript --check \"src/**/*.{ts,tsx}\"",
    "format:fix": "prettier --parser typescript --write \"src/**/*.{ts,tsx}\"",
    "lint": "tslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "tslint --fix \"src/**/*.{ts,tsx}\"",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.7.5",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.2"
  }
}
